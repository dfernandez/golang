<table class="table table-striped">
<tr>
	<th>Id</th>
	<th>Name</th>
	<th>Email</th>
	<th>First login</th>
	<th>Is admin</th>
</tr>
{{ range $index, $profile := .Get "profiles" }}
<tr>
	<td>{{ $profile.ID }}</td>
	<td>{{ $profile.Name }}</td>
	<td>{{ $profile.Email }}</td>
	<td>{{ $profile.FirstLogin }}</td>
	<td>{{ if $profile.Admin }}<span class="glyphicon glyphicon-ok" aria-hidden="true"></span>{{ end }}</td>
</tr>
{{ end }}
</table>