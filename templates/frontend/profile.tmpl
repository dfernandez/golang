<div class="profile-container">
    <img src="{{ (.Get "profile").Picture }}" class="img-thumbnail profile-image pull-left" />
    <div>
        <h1 class="profile-h1">{{ (.Get "profile").Name }}</h1>
        <p>{{ (.Get "profile").Email }}</p>
        <p>{{ (.Get "profile").GetCreationDate }}</p>
    </div>
</div>
